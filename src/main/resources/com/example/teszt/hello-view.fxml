<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="1200.0" styleClass="window-defaults" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.teszt.HelloController">
   <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
     <tabs>
       <Tab id="productsTab" closable="false" text="TERMÉKEK">
         <content>
           <AnchorPane styleClass="content-wrapper">
                  <children>
                     <VBox alignment="CENTER" spacing="16.0" styleClass="content-wrapper" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <children>
                           <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="30.0" VBox.vgrow="NEVER">
                              <children>
                                 <VBox alignment="CENTER_LEFT" spacing="8.0" HBox.hgrow="ALWAYS">
                                    <children>
                                       <Label styleClass="form-label" text="TERMÉK-MŰVELETEK" />
                                       <HBox fillHeight="false" spacing="8.0" VBox.vgrow="NEVER">
                                          <children>
                                             <Button mnemonicParsing="false" onAction="#updatemeals" styleClass="btn-primary" text="Újratöltés" />
                                             <Button fx:id="delete_meal" mnemonicParsing="false" onAction="#deleteSelectedMeal" styleClass="btn-primary" text="Törlés" />
                                             <Button fx:id="edit_button" mnemonicParsing="false" styleClass="btn-primary" text="Szerkesztés" />
                                              <Button fx:id="add_button" mnemonicParsing="false" styleClass="btn-primary" text="Új Termék" />
                                          </children>
                                       </HBox>
                                    </children>
                                 </VBox>
                              </children>
                           </HBox>
                            <TableView fx:id="Menu_table" VBox.vgrow="ALWAYS">
                                <placeholder>
                                    <Label text="Nincsenek megjeleníthető elemek..." />
                                </placeholder>
                            </TableView>
                        </children>
                     </VBox>
                  </children>
               </AnchorPane>
         </content>
       </Tab>
         <Tab id="ordersTab" closable="false" text="RENDELÉSEK">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="content-wrapper">
                  <children>
                     <VBox alignment="CENTER" spacing="16.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <children>
                           <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="30.0" styleClass="content-wrapper" VBox.vgrow="NEVER">
                              <children>
                                 <VBox alignment="CENTER_LEFT" spacing="8.0" HBox.hgrow="NEVER">
                                    <children>
                                       <Label styleClass="form-label" text="RENDELÉS-MŰVELETEK" />
                                       <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="8.0" VBox.vgrow="NEVER">
                                          <children>
                                             <Button mnemonicParsing="false" onAction="#updateorders" styleClass="btn-primary" text="Újratöltés" />
                                             <Button fx:id="order_delete" mnemonicParsing="false" onAction="#deleteSelectedOrder" styleClass="btn-primary" text="Törlés" />
                                             <Button fx:id="order_edit" mnemonicParsing="false" styleClass="btn-primary" text="Szerkesztés" />
                                          </children>
                                       </HBox>
                                    </children>
                                 </VBox>
                                 <VBox spacing="8.0" HBox.hgrow="ALWAYS">
                                    <children>
                                       <Label styleClass="form-label" text="RENDELÉS TÉTEL-MŰVELETEK" />
                                       <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="8.0" VBox.vgrow="NEVER">
                                          <children>
                                             <Button fx:id="orderitem_delete" mnemonicParsing="false" onAction="#deleteSelectedOrderItem" styleClass="btn-primary" text="Törlés" />
                                             <Button fx:id="orderitem_edit" mnemonicParsing="false" styleClass="btn-primary" text="Szerkesztés" />
                                             <Button fx:id="orderitem_add" mnemonicParsing="false" onAction="#addOrderItem" styleClass="btn-primary" text="Új Tétel" />
                                          </children>
                                       </HBox>
                                    </children>
                                 </VBox>
                              </children>
                           </HBox>
                           <TableView fx:id="Orders_table" prefHeight="230.0" prefWidth="772.0" VBox.vgrow="ALWAYS">
                               <placeholder>
                                   <Label text="Nincsenek megjeleníthető elemek..." />
                               </placeholder>
                           </TableView>
                           <TableView fx:id="Orderitem_table" prefHeight="230.0" prefWidth="772.0" VBox.vgrow="ALWAYS">
                               <placeholder>
                                   <Label text="Nincsenek megjeleníthető elemek..." />
                               </placeholder>
                           </TableView>
                        </children>
                     </VBox>
                  </children>
               </AnchorPane>
           </content>
         </Tab>
       <Tab id="usersTab" closable="false" text="FELHASZNÁLÓK">
            <content>
               <AnchorPane styleClass="content-wrapper">
                  <children>
                     <VBox spacing="16.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <children>
                           <HBox fillHeight="false" spacing="30.0" styleClass="content-wrapper" VBox.vgrow="NEVER">
                              <children>
                                 <VBox alignment="CENTER_LEFT" spacing="8.0" HBox.hgrow="ALWAYS">
                                    <children>
                                       <Label styleClass="form-label" text="FELHASZNÁLÓ-MŰVELETEK" />
                                       <HBox fillHeight="false" spacing="8.0" VBox.vgrow="NEVER">
                                          <children>
                                             <Button mnemonicParsing="false" onAction="#updateusers" styleClass="btn-primary" text="Újratöltés" />
                                             <Button fx:id="user_delete" mnemonicParsing="false" onAction="#deleteSelectedUser" styleClass="btn-primary" text="Törlés" />
                                             <Button fx:id="user_edit" mnemonicParsing="false" styleClass="btn-primary" text="Szerkesztés" />
                                          </children>
                                       </HBox>
                                    </children>
                                 </VBox>
                              </children>
                           </HBox>
                           <TableView fx:id="User_table">
                               <placeholder>
                                   <Label text="Nincsenek megjeleníthető elemek..." />
                               </placeholder>
                           </TableView>
                        </children>
                     </VBox>
                  </children>
               </AnchorPane>
            </content>
       </Tab>
         <Tab id="infoTab" text="NÉVJEGY">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="content-wrapper">
                  <children>
                     <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="18.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <children>
                           <VBox VBox.vgrow="NEVER">
                              <children>
                                 <Label styleClass="heading" text="Erről az alkalmazásról" VBox.vgrow="ALWAYS" />
                                 <Label text="Ez az alkalmazás a 2025-ös &quot;Éttermi Rendelő Rendszer&quot; c. szakmai vizsgaremekünk részét képezi." />
                                 <Label text="A forráskód megtekinthető GitHub-on, a  https://github.com/creppzguy/Viszgaremek_Admin címen." />
                                 <Label text="A projekthez tartozó webes felület elérhető a https://vizsgaremek.halaszbendeguz.hu oldalon." />
                              </children>
                           </VBox>
                           <VBox>
                              <children>
                                 <Label styleClass="heading" text="Csapat" />
                                 <Label text="Készítették:" />
                                 <Label text="- Kőszegi Tamás Attila (fejlesztés)" />
                                 <Label text="- Simon Attila Tibor (fejlesztés, tervezés)" />
                                 <Label text="- Halász Wilhelm Bendegúz (tervezés, dizájn)" />
                              </children>
                           </VBox>
                        </children>
                     </VBox>
                  </children>
               </AnchorPane>
           </content>
         </Tab>
     </tabs>
   </TabPane>
</VBox>
